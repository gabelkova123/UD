package lab_2;
import java.util.Scanner;

public class Task_2 {

    public static void main(String[] args) {



        Scanner in = new Scanner(System.in);

        System.out.println("enter n: ");

        int n = in.nextInt();
        int result = 1;

        for (int i = 1; i <= n; i++)
        {
            result *=i;
            System.out.println("fact" + i +"* " + result);
        }
    }
}
