
package Lab_2;
        import java.util.Scanner;

class Task_1 {


    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("enter n: ");
        int n = in.nextInt();
        int rez = 0;
        for (int i = 0; i < n; i++) {
            rez = (int) Math.pow (2, n);
        }
        System.out.println(rez);
    }
}
